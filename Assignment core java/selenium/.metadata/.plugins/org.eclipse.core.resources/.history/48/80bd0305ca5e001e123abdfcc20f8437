package Opps;

class std
{
	private int id;
	private String name;
	private String email;
	private int phno;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public int getPhno() {
		return phno;
	}
	public void setPhno(int phno) {
		this.phno = phno;
	}
}

public class O006_Encapsulation_Demo {

	public static void main(String[] args) {
		
	 std std = new std();
	 
	 std.getId();
	 std.getName();
	 std.getEmail();
	 std.getPhno();
		
	}
}
