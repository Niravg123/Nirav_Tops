package Petttern;

public class Pettern4 {
public static void main(String[] args) {
	
	int lines = 5;
	int starcount = 1;
	int spacecount = lines-1;

	for(int j=1;j<=lines;j++)
	{
		for(int k=1;k<=spacecount;k++)
		{
			System.out.println(" ");
		}
		for(int i=1;i<=starcount;i++)
		{
			System.out.println("*");
		}
		System.out.println();
		starcount = starcount+2;
			spacecount--;
	}
}
}
